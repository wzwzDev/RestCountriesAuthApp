        <activity android:name=".CountriesAuth.Common.LoginSignup.RetailerStartUpScreen" />
        <activity android:name=".CountriesAuth.User.UserDashboard" />
        <activity
            android:name=".CountriesAuth.Common.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>